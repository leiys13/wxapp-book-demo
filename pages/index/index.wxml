<!--pages/index/index.wxml-->
<map id="map" 
  bindcontroltap="tapControl"
  bindregionchange="changeRegion"
  longitude="{{longitude}}" 
  latitude="{{latitude}}"
  markers="{{markers}}"
  controls="{{controls}}"
  style="width:{{mapw}};height:{{maph}};"
  scale="{{scale}}"
  show-location >
</map>
